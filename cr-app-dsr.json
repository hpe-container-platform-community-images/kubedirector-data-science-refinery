{
    "apiVersion": "kubedirector.hpe.com/v1beta1",
    "kind": "KubeDirectorApp",
    "metadata": {
        "name" : "dsr"
    },
    "spec" : {
        "systemdRequired": false,
        "defaultPersistDirs" : ["/home"],
        "defaultEventList" : ["configure"],
        "config": {
            "roleServices": [
                {
                    "serviceIDs": [
                        "http"
                    ],
                    "roleID": "node"
                }
            ],
            "selectedRoles": [
                "node"
            ]
        },
        "label": {
            "name": "Data Science Refinery",
            "description": "Data Science Refinery"
        },
        "distroID": "kubedirector/dsr",
        "version": "0.1",
        "configSchemaVersion": 7,
        "services": [
            {
                "endpoint": {
                    "urlScheme": "http",
                    "path": "/",
                    "port": 9995,
                    "isDashboard": true
                },
                "id": "http",
                "label": {
                    "name": "http"
                }
            }
        ],
        "defaultImageRepoTag": "snowch/kubedirector-data-science-refinery:latest",
        "defaultConfigPackage": {
            "packageURL": "https://github.com/hpe-container-platform-community-images/kubedirector-data-science-refinery/releases/download/refs%2Fheads%2Fmain/appconfig.tgz"
        },
        "roles": [
            {
                "cardinality": "1",
                "id": "node",
                "eventList": ["configure"]
            }
        ]
    }
}
